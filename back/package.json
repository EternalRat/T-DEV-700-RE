{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "build": "tsc --build",
    "lint": "npx eslint ./src --ext .js,.ts",
    "lint:fix": "npx eslint ./src --ext .js,.ts,.tsx --fix",
    "format": "prettier --write \"./src/**/*.{js,ts,md,json}\"",
    "test:unit": "vitest --coverage.enabled true -c ./vitest.config.unit.ts",
    "test:unit:ui": "vitest -c ./vitest.config.unit.ts --ui",
    "docs": "typedoc --out ../docs ./src/**/*.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "author": "HENRY Benjamin",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.14.0",
    "@vitest/coverage-istanbul": "1.6.0",
    "bcrypt": "5.1.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "mysql": "2.18.1",
    "passport-jwt": "4.0.1",
    "prettier": "2.8.8",
    "prisma": "5.14.0",
    "socket.io": "4.5.4",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.9",
    "@types/passport-jwt": "3.0.7",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.5.0",
    "module-alias": "2.2.3",
    "nodemon": "3.1.0",
    "ts-node": "10.9.2",
    "typedoc": "0.25.13",
    "typescript": "5.4.5",
    "vitest": "1.5.0",
    "vitest-mock-extended": "1.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript",
    "tabWidth": 4,
    "useTabs": true,
    "bracketSameLine": true,
    "semi": true,
    "endOfLine": "auto",
    "jsxSingleQuote": true,
    "quoteProps": "as-needed",
    "printWidth": 80
  }
}